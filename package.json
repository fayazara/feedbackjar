{
  "name": "feedbackjar",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "format": "prettier --write .",
    "format:diff": "prettier --check .",
    "lint": "npm run lint:vue && npm run lint:style",
    "lint:ci": "npm run lint:vue --quiet && npm run lint:style",
    "lint:vue": "eslint --cache --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:fix": "npm run lint:eslint-fix && npm run lint:style-fix",
    "lint:eslint-fix": "eslint --fix --cache --report-unused-disable-directives \"**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:style-fix": "stylelint --fix \"**/*.css\"",
    "lint:style": "stylelint \"**/*.css\"",
    "db:migrate": "drizzle-kit generate:sqlite",
    "db:deploy": "drizzle-kit push:sqlite"
  },
  "devDependencies": {
    "@nuxt/devtools": "0.7.4",
    "@nuxthq/ui": "2.7.0",
    "@nuxtjs/fontaine": "0.4.1",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^5.59.9",
    "@vueuse/core": "10.3.0",
    "@vueuse/nuxt": "10.3.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nuxt": "3.6.5",
    "nuxt-icon": "^0.4.2",
    "prettier": "^2.8.4",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "*"
  },
  "dependencies": {
    "@iconify-json/mdi": "^1.1.52",
    "@libsql/client": "0.3.1",
    "better-sqlite3": "8.5.0",
    "bufferutil": "^4.0.7",
    "drizzle-kit": "0.19.12",
    "drizzle-orm": "^0.27.0",
    "h3-zod": "^0.5.1",
    "utf-8-validate": "^6.0.3",
    "zod": "^3.21.4"
  }
}
